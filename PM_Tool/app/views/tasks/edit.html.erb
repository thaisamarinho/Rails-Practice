<%= @task.errors.full_messages.join(', ')%>
<%= form_for [@project, @task] do |f| %>
  <div>
    <%= f.label :title%>
    <%= f.text_field :title%>
  </div>
  <br>
  <div>
    <%= f.label :body%>
    <%= f.text_area :body%>
  </div>
  <div>
    <%= f.label :due_date%>
    <%= f.date_field :due_date%>
  </div>
  <br>
  <div>
  <%= f.submit%>
  </div>
<% end %>
